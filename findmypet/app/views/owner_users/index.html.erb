<p id="notice"><%= notice %></p>

<h1>Listing Owner Users</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owner_users.each do |owner_user| %>
      <tr>
        <td><%= owner_user.owner %></td>
        <td><%= owner_user.user %></td>
        <td><%= link_to 'Show', owner_user %></td>
        <td><%= link_to 'Edit', edit_owner_user_path(owner_user) %></td>
        <td><%= link_to 'Destroy', owner_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Owner user', new_owner_user_path %>
